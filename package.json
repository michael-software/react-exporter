{
  "name": "react-exporter",
  "version": "0.0.1",
  "description": "A react-component exporter",
  "main": "lib/index.js",
  "scripts": {
    "start": "cross-env BABEL_ENV=development webpack-dev-server --progress --config ./webpack/dev.babel.js",
    "build": "babel src/ReactExporter.js --presets es2015,stage-0 --out-file lib/index.js",
    "build:example": "cross-env BABEL_ENV=production webpack -p --colors --progress --config ./webpack/prod.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelsoftware1997/react-exporter.git"
  },
  "author": "Michael Braun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaelsoftware1997/react-exporter/issues"
  },
  "homepage": "https://github.com/michaelsoftware1997/react-exporter#readme",
  "dependencies": {
    "node-sass": "^4.1.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-hot": "^1.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.11",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^4.1.1",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
